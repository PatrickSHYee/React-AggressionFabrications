        </symbol>
    </svg>
    <div class="container-fluid bg-dark">
        <header class="d-flex flex-wrap align-items-center justify-content-center justify-content-md-between py-3 mb-4">
            <a class="d-flex align-items-center col-md-3 mb-2 mb-md-0 text-white navbar-brand text-white text-decoration-none" asp-area="" asp-controller="Home" asp-action="Index">React Aggression Fabrications</a>

            <ul class="nav col-12 col-md-auto mb-2 justify-content-center mb-md-0">
                <li><a asp-area="" asp-controller="Home" asp-action="Index" class="nav-link px-2 link-secondary">
                    <svg width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1">
                        <use xlink:href="#home"></use>
                    </svg>
                    Home</a>
                </li>
                <li>
                    <a class="nav-link px-2 link-light" asp-area="" asp-controller="Vehicles" asp-action="Index">
                        <svg width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1">
                            <use xlink:href="#armor"></use>
                        </svg>
                        Vehicles</a>
                </li>
                <li>
                    <a class="nav-link px-2 link-light" asp-area="" asp-controller="Projects" asp-action="Index">
                        <svg width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1">
                            <use xlink:href="#shop"></use>
                        </svg>
                        Projects</a>
                </li>
                <li>
                    <a class="nav-link px-2 link-light" asp-area="" asp-controller="Merch" asp-action="Index">
                        <svg width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1">
                            <use xlink:href="#merch"></use>
                        </svg>
                        Merch</a>
                </li>
                <li>
                    <a class="nav-link px-2 link-light" asp-area="" asp-controller="FAQ" asp-action="Index">
                        <svg width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1">
                            <use xlink:href="#question"></use>
                        </svg>
                        Need Help</a>
                </li>
            </ul>
            <div class="d-flex align-items-center">
                <form class="w-100 me-3">
                    <div class="input-group">
                        <input type="search" class="form-control" placeholder="Search..." aria-label="Search">
                        <button class="btn btn-light" type="button" id="button-addon2">
                            <svg width="16" height="16" fill="#000000" class="bi d-block mx-auto mb-1">
                                <use xlink:href="#search"></use>
                            </svg></button>
                    </div>
                </form>
                <div class="flex-shrink-0">
                    <a class="nav-link link-light text-end" asp-area="" asp-controller="" asp-action="">
                        <svg width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1">
                            <use xlink:href="#checkout"></use>
                        </svg>
                    </a>
                </div>
            </div>
        </header>
    </div>
    <div class="px-3 py-1 mb-3">
        <partial name="_LoginPartial"/>
    </div>
        
        @*<header class="d-flex flex-wrap align-content-center justify-content-center justify-content-md-between py-3 mb-4">
            <div class="px-3 py-2 bg-dark">
                <div class="container">
                    <div class="col-md-3 text-end">
                        <form class="col-3 col-lg-auto mb-3 mb-lg-0 me-lg-3">
                            <input type="search" class="form-control form-control-sm" placeholder="Search" aria-label="Search" />
                        </form>
                        <a class="nav-link text-white" asp-area="" asp-controller="">
                            <svg width="24" height="24" fill="currentColor" class="bi d-block mx-auto mb-1">
                                <use xlink:href="#checkout"></use>
                            </svg>
                        </a>
                    </div>
                </div>
            </div>
        </header>*@
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - React_AggressionFabrications - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
